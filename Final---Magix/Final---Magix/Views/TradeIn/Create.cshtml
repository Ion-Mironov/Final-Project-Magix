@model IEnumerable<Final___Magix.Models.TradeInModel>

@{
    ViewData["Title"] = "Create";
}

	<!-- Main container that handles background image and all content within it. -->
	<div class="tradein-container">

		<div class="alpha-tradein-wrapper">
			<div class="tradein-greeting-text-image">
				<img src="/Images/tradein_greeting.png" alt="Trade-In Greeting Text">
			</div>

			<!-- The 'form' element has its own Bootstrap template which causes styling conflicts. Giving it a class overrides its default template. -->
			<form class="tradein-form-wrapper" method="post" asp-action="AddToTradeIn">
				<div class="cardNameWrapper">
					<!-- <div> template for labels with tooltips -->
					<div class="label-with-tooltip">
						<label for="cardName">Card Name</label><span class="tooltip-icon" onclick="openTooltip('tooltip-cardName')">?</span>
					</div>



				<div class="matching-cards">
					<h3>Matching Cards:</h3>
					@if (Model != null)
					{
						<ul>
							@foreach (var card in Model)
							{
								<li>@card.cardName</li>
							}
						</ul>
					}
					else
					{
						<p>No matching cards found.</p>
					}
				</div>

					


					<!-- Tooltip modal for the Card Name field; triggered on click. See /wwwroot/js/site.js for JavaScript code. -->
					<div class="tooltip-modal" id="tooltip-cardName">

						<!-- Content of the tooltip; styled and positioned as a tooltip box -->
						<div class="tooltip-content tooltip-content-box">
							Search by entering any word(s) found in the card title.<br>
							Partial matches will also provide results.
						</div>
					</div>
					<input type="text" id="cardName" name="cardName" required>
				</div>
				<div class="cardEditionWrapper">
					<!-- <div> template for labels with tooltips -->
					<div class="label-with-tooltip">
						<label for="cardEdition">Edition</label><span class="tooltip-icon" onclick="openTooltip('tooltip-cardEdition')">?</span>
					</div>

					<!-- Tooltip modal for the Card Edition field; triggered on click. See /wwwroot/js/site.js for JavaScript code. -->
					<div class="tooltip-modal" id="tooltip-cardEdition">

						<!-- Content of the tooltip; styled and positioned as a tooltip box -->
						<div class="tooltip-content tooltip-content-box">
							Hope you like scrolling! This list contains every set in Magic: The Gathering history.
						</div>
					</div>

				

					@{
						var bulkDataEntries = ViewBag.BulkDataEntries as List<BulkData>;
					}

					<!-- Card Name Validation -->
					<input type="text" id="cardNameInput" placeholder="Enter card name" oninput="validateCardName()">

					<!-- Print Dropdown -->
					<select id="printDropdown"></select>

					<script>
						function validateCardName() {
							const cardNameInput = document.getElementById("cardNameInput").value;

							const matchingCards = bulkDataEntries.filter(card => card.Name === cardNameInput);

							const printDropdown = document.getElementById("printDropdown");
							printDropdown.innerHTML = ""; // Clear existing options

							if (matchingCards.length > 0) {
								matchingCards.forEach(card => {
									const option = document.createElement("option");
									option.value = card.Id; // You might want to use a unique identifier here
									option.textContent = card.Name; // Adjust this based on the property name in BulkData model
									printDropdown.appendChild(option);
								});
							} else {
								const option = document.createElement("option");
								option.value = ""; // You can set a default value for no match
								option.textContent = "No matching card found";
								printDropdown.appendChild(option);
							}
						}
					</script>

					<!-- Existing form elements -->
					<!-- Display matching cards -->
					@if (ViewBag.MatchingCards != null && ViewBag.MatchingCards.Count() > 0)
					{
						<h4>Matching Cards:</h4>
						<ul>
							@foreach (var card in ViewBag.MatchingCards)
							{
								<li>@card.cardName</li>
							}
						</ul>
					}



